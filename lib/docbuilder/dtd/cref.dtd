<!-- `name' is used in common.refs.dtd and must therefore 
      be defined in each *ref. dtd -->
<!ELEMENT name                (ret,nametext) >
<!ELEMENT ret                 (#PCDATA) >
<!ELEMENT nametext            (#PCDATA) >

