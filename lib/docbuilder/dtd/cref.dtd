<?xml version="1.0" encoding="utf-8" ?>

<!ENTITY % common.refs SYSTEM "common.refs.dtd" >
%common.refs;

<!ELEMENT cref                (header,lib,libsummary,description, 
			       (section|funcs)*,authors) >
